<!doctype html>
<html data-ng-app="GoClimb">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<title>User Admin</title>

	<link rel="stylesheet" href="{$basePath}/assets/admin.css">
</head>
<body data-zf-close-all>

<div class="vertical grid-frame">
	<div data-top-panel></div>
	<div class="grid-block">
		<div data-admin-menu></div>
		<div class="grid-block vertical" style="display: block">
			<div data-admin-menu-button></div>

			{*<div data-zf-panel id="sidebar" position="right" class="medium-grid-block medium-3 vertical is-active">*}
			<div class="divider primary grid-block">
				<div class="grid-container grid-block">
					<h1 class="title">Obecné</h1>
					<ul class="menu-bar right icon-top">
						<li class="menu-item"><a class="link" href="#"><span class="fa fa-user"></span> example</a></li>
						<li class="menu-item"><a class="link" zf-toggle="sidebar"><span class="fa fa-user"></span>
								Toggle</a></li>
					</ul>
				</div>
			</div>
			<form>

				<div class="divider grid-block">
					<div class="grid-container grid-block">
						<h2 class="title">Obecné</h2>
					</div>
				</div>
				<div class="content">
					<div class="grid-container grid-block wrap">
						<label class="small-12">
							Název
							<input type="text" placeholder="Text field">
						</label>
					</div>
				</div>

				<div class="divider">
					<div class="grid-container grid-block">
						<h2 class="title">Image</h2>
					</div>
				</div>
				<div class="content">
					<div class="grid-container grid-block wrap">
						<label class="small-12">
							Input Label
							<input type="file" placeholder="Text field">
						</label>
					</div>
				</div>

				<div class="divider">
					<div class="grid-container grid-block">
						<h2 class="title">Danger zone</h2>
						<ul class="menu-bar right">
							<li class="menu-item"><a class="link" href="#"><span
											class="fa fa-phone"> asistence</span></a></li>
						</ul>
					</div>
				</div>
				<div class="content">
					<div class="grid-container grid-block wrap noscroll">
						<div>
							<label for="switch-example"> Disable</label>
							<span class="switch">
								<input type="checkbox" id="switch-example">
								<label for="switch-example"> </label>
							</span>
						</div>
					</div>
				</div>
			</form>
		</div>
		{*<div class="grid-block medium-3 show-for-medium vertical">*}
		<div data-zf-panel id="sidebar" position="right" class="medium-grid-block medium-3 vertical is-active">
			<div class="divider primary grid-block">
				<div class="grid-container grid-block">
					<h1 class="title">Obecné</h1>

				</div>
			</div>
			<div class="divider menu grid-block">
				<ul class="menu-bar right">
					<li class="menu-item"><a class="link" href="#"><span class="fa fa-user"></span> example</a></li>
					<li class="menu-item"><a class="link" zf-toggle="sidebar"><span class="fa fa-user"></span>
							Toggle</a></li>
				</ul>
			</div>
			<form>

				<div class="divider grid-block">
					<div class="grid-container grid-block">
						<h2 class="title">Obecné</h2>
					</div>
				</div>
				<div class="content">
					<div class="grid-container grid-block wrap">
						<label class="small-12">
							Název
							<input type="text" placeholder="Text field">
						</label>
					</div>
				</div>

				<div class="divider">
					<div class="grid-container grid-block">
						<h2 class="title">Image</h2>
					</div>
				</div>
				<div class="content">
					<div class="grid-container grid-block wrap">
						<label class="small-12">
							Input Label
							<input type="file" placeholder="Text field">
						</label>
					</div>
				</div>

				<div class="divider">
					<div class="grid-container grid-block">
						<h2 class="title">Danger zone</h2>
						<ul class="menu-bar right">
							<li class="menu-item"><a class="link" href="#"><span
											class="fa fa-phone"> asistence</span></a></li>
						</ul>
					</div>
				</div>
				<div class="content">
					<div class="grid-container grid-block wrap noscroll">
						<div>
							<label for="switch-example"> Disable</label>
							<span class="switch">
								<input type="checkbox" id="switch-example">
								<label for="switch-example"> </label>
							</span>
						</div>
					</div>
				</div>
			</form>
		</div>
	</div>
</div>

<script src="{$basePath}/assets/admin.js"></script>
</body>
</html>
