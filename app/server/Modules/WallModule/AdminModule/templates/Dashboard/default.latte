<!doctype html>
<html data-ng-app="GoClimb">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<base href="{link this, 'path' => NULL}">

	<title>User Admin</title>

	<link rel="stylesheet" href="{$basePath}/assets/admin.css">
</head>
<body data-zf-close-all>

	<div data-app>
		Loading...
	</div>

	<script src="{$basePath}/assets/admin.js"></script>
	<script n:syntax="double">(function(a,m,s,l){a.module(m).config([s,function(t){t.preferredLanguage(l).fallbackLanguage(l)}])})(angular,'GoClimb','$translateProvider',{{$locale}})</script>
	<script n:syntax="double">(function(a,m,s,t){a.module(m).constant(s,{{$availableLanguages}}).constant(t,{{plink //:Wall:Rest:V1:Dashboard:default}})})(angular,'GoClimb','availableLanguages','apiUrl')</script>
</body>
</html>
