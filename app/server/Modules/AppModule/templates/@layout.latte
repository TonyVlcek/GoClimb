<!doctype html>
<html data-ng-app="GoClimb">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<base href="{link this, 'path' => NULL}">

	<title>{_templates.app.layout.title} &middot; {_templates.core.websiteName}</title>

	<link rel="stylesheet" n:cdnHref="assets/app.css">
</head>
<body>
{include ../../templates/includes/topPanel.latte}
<div class="loading-box" data-app>
	{include ../../templates/includes/loading.latte}
</div>

<script n:cdnSrc="assets/app.js"></script>
<script n:syntax="double">(function (a, m, s, l){a.module(m).config([s,function(t){t.preferredLanguage(l).fallbackLanguage(l)}])})(angular, 'GoClimb', '$translateProvider',{{$locale}})</script>
<script n:syntax="double">(function (a, m, d){var g=a.module(m),k;for(k in d){d.hasOwnProperty(k)?g.constant(k,d[k]):0;}})(angular, 'GoClimb',{{$data}})</script>
</body>
</html>
